{
  "name": "@asyncapi/glee",
  "version": "0.1.0",
  "description": "The AsyncAPI framework",
  "main": "src/moduleIndex.js",
  "type": "module",
  "bin": {
    "glee": "./src/cli/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:runtime:client": "ag ./GleeRuntime.json ./templates/ClientTemplate --force-write --output ./src/runtime/client && npm run generate:runtime:proto:client",
    "generate:runtime:java": "ag ./GleeRuntime.json ./templates/ClientTemplate --force-write --output ./src/runtime/client && npm run generate:runtime:proto:java",
    "generate:runtime:proto:client": "ag ./GleeRuntime.json ./templates/ProtoTemplate --force-write --output ./src/runtime/client/proto",
    "generate:runtime:proto:java": "ag ./GleeRuntime.json ./templates/ProtoTemplate --force-write --output ./GleeJavaRuntime/src/main/proto/asyncapi/glee"
  },
  "keywords": [
    "asyncapi",
    "framework"
  ],
  "author": "Fran Mendez",
  "license": "Apache-2.0",
  "devDependencies": {
    "@asyncapi/generator": "^1.7.4"
  },
  "dependencies": {
    "@asyncapi/modelina": "^0.14.1",
    "@asyncapi/parser": "^1.5.0",
    "@grpc/grpc-js": "^1.3.4",
    "@grpc/proto-loader": "^0.6.3",
    "ajv": "^6.12.6",
    "async": "^3.2.0",
    "better-ajv-errors": "^0.7.0",
    "bufferutil": "^4.0.3",
    "chalk": "^4.1.1",
    "debug": "^4.3.1",
    "emojis": "^1.0.10",
    "localenv": "^0.2.2",
    "mqtt": "^4.2.6",
    "nodemon": "^2.0.7",
    "path-to-regexp": "^6.2.0",
    "socket.io": "^4.1.2",
    "terminal-image": "^2.0.0",
    "utf-8-validate": "^5.0.5",
    "walkdir": "^0.4.1",
    "word-wrap": "^1.2.3",
    "ws": "^7.4.6"
  }
}
